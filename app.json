{
  "name": "izumi-bot",
  "description": "Izumi a Multi Device whatsapp bot",
  "logo": "https://i.imgur.com/Hgiamot.jpeg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/Akshay-Eypz/izumi-bot",
  "website": "https://github.com/Akshay-Eypz/izumi-bot",
  "success_url": "https://github.com/Akshay-Eypz/izumi-bot",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "enter your session id",
      "required": true
    },
     "MENU_URL": {
      "description": " ",
      "required": "false",
      "value": "https://i.imgur.com/8H2hM6D.jpeg"
     },
     "BOT_NAME": {
      "description": "enter your bot name",
      "required": "false",
      "value": "Iᴢᴜᴍɪ-ᴍᴅ"
    },
       "STICKER_PACKNAME": {
      "description": "enter your sticker pack name",
      "required": "false",
      "value": "Iᴢᴜᴍɪ-ᴍᴅ,❤️"
    }, "AUDIO_DATA": {
      "description": "enter your audio data(title,author,link)",
      "required": "false",
      "value": "enjoy this song,Iᴢᴜᴍɪ-v3,https://i.imgur.com/cO0TZJv.jpeg"
    },
       "OWNER_NAME": {
       "description": "Enter your name"
       "required": "false",
       "value": "Eʏᴘᴢ"
    },
     "AUTHOR": {
      "description": "enter sticker pack author name ",
      "required": "false",
      "value": "Eypz"
    },
    "SUDO": {
      "description": "add sudo",
      "required": "false",
      "value": "91xxxxxxxxxx",
    },
    "MODE": {
      "description": "bot work mode",
      "required": "true",
      "value": "private"
     },
    "HEROKU": {
      "description": "Don't change this",
      "required": true,
       "value":"true"
     },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
     }
 },
 "addons": [{
      "plan": "heroku-postgresql"
    }],
    "buildpacks": [{
      "url": "heroku-community/apt"
      },
      {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
      }]
  }
